<!DOCTYPE html>
<html lang="en">

    <head>
        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <meta name="format-detection" content="telephone=no">
        <meta name="title" content="@ViewData["MetaTitle"] - @_tenantProvider.Tenant?.Name">
        <meta name="description" content="This is the personal website website of @_tenantProvider.Tenant?.Name and serves as as his minimalistic online presence.">
        <meta name="author" content="Eric Jansen">
        <meta name="robots" content="follow, index" />
        <title>@ViewData["Title"] - @_tenantProvider.Tenant?.Name</title>
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
        <link rel="stylesheet" href="~/css/ericjansen.css" asp-append-version="true" />
    </head>

    <body>

        <div class="wrapper">

            <div class="content mb-4">

                <partial name="_GdprConsent" />

                <div class="container">
                    <partial name="_Header" />
                    @RenderBody()
                </div>

            </div>

            <partial name="_Footer" />

        </div>

        <script src="https://code.jquery.com/jquery-3.7.1.min.js" integrity="sha256-/JqT3SQfawRcv/BIHPThkBvs0OEvtFFmqPF/lYI/Cxo=" crossorigin="anonymous"></script>
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>
        <script src="~/js/js.cookie.min.js" asp-append-version="true"></script>
        <script src="~/js/ericjansen.js" asp-append-version="true"></script>

    </body>

</html>